{
  "package": "com.application.demo",
  "name": "quickapp",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/Common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.fetch"},
    { "name": "system.request"},
    { "name": "system.media'"},
    { "name": "system.image" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "Scan",
    "pages": {
      "Scan": {
        "component": "index"
      },
      "Demo": {
        "component": "index"
      },
      "DemoDetail": {
        "component": "index"
      },
      "About": {
        "component": "index"
      }
    },
    "widgets": {
      "CardDemo": {
        "name": "CardDemo",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/CardDemo",
        "features": []
      }
    }
  },
  "display": {
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      },
      "DemoDetail": {
        "titleBarText": "详情页"
      },
      "About": {
        "menu": false
      },
      "Scan": {
        "menu": false,
        "titleBar": false
      }
    },
    "windowSoftInputMode": "adjustResize"
  }
}